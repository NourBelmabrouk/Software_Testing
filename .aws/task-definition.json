{
    "family": "nour-tsk-def-conference-app-pipeline",
    "containerDefinitions": [
        {
            "name": "conference-app",
            "image": "nourbelmabrouk/conference-app",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256"
}